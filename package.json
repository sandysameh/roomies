{
  "name": "roomies",
  "version": "1.0.0",
  "description": "Video calling application with Daily.js",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm start",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "video",
    "calling",
    "daily.js",
    "react",
    "typescript"
  ],
  "author": "FullyRamped",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
